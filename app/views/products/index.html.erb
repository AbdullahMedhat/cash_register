<div class="body">
  <p style="color: green"><%= notice %></p>

  <h1>Products</h1>
  <div class="container">
    <div id="products">
      <% @products.each do |product| %>
        <%= render product %>
      <% end %>
    </div>

  <div id="basket">
    <%= render "basket", order: @order %>
  </div>

</div>
